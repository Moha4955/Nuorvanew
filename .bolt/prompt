Rate Structure

SCHADS_BASE_RATES = {
  level1: 38.50,  // Entry level
  level2: 42.00,  // Experienced worker  
  level3: 45.50,  // Senior worker
  level4: 48.75,  // Specialist/Team leader
  level5: 52.25   // Coordinator/Manager
}

PENALTY_RATES = {
  evening: 1.25,    // 6PM - 10PM
  night: 1.50,      // 10PM - 6AM
  saturday: 1.25,   // All day Saturday
  sunday: 1.50,     // All day Sunday
  public_holiday: 2.00, // Public holidays
  overtime: 1.50    // Over 8 hours per day
}
